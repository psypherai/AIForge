{
  "name": "@aiforge/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "uv run uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "build": "uv sync",
    "lint": "uv run ruff check .",
    "typecheck": "uv run mypy app/",
    "test": "uv run pytest",
    "clean": "rm -rf .venv __pycache__ .mypy_cache .ruff_cache .pytest_cache"
  }
}
