{
  "name": "aiforge-starter-pack",
  "version": "1.0.0",
  "private": true,
  "description": "The #1 AI-native monorepo template for 2026+. Next.js + Expo + FastAPI + LangGraph + Supabase with 90%+ code sharing.",
  "author": "Psypher AI <hello@psypher.ai>",
  "license": "MIT",
  "homepage": "https://psypher.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/psypherai/AIForge.git"
  },
  "bugs": {
    "url": "https://github.com/psypherai/AIForge/issues"
  },
  "keywords": [
    "ai",
    "monorepo",
    "nextjs",
    "expo",
    "react-native",
    "fastapi",
    "langgraph",
    "supabase",
    "tailwind",
    "nativewind",
    "turborepo",
    "rag",
    "pgvector",
    "vercel-ai-sdk",
    "langfuse",
    "starter-template",
    "boilerplate"
  ],
  "packageManager": "pnpm@10.30.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:web": "turbo run dev --filter=@aiforge/web",
    "dev:mobile": "turbo run dev --filter=@aiforge/mobile",
    "dev:backend": "turbo run dev --filter=@aiforge/backend",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@aiforge/web",
    "lint": "turbo run lint",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "api:generate": "turbo run generate --filter=@aiforge/api-client",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "prepare": "husky"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "supabase",
      "unrs-resolver"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "husky": "^9.1.7",
    "turbo": "^2.8.9",
    "typescript": "^5.9.3"
  }
}
